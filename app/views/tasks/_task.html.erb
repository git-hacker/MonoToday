<li id="task-id-<%= task.id %>">
  <span><%= task.title %></span>
  <div class="actions">
    <% if !task.completed? %>
      <%= link_to complete_task_path(task), method: 'POST', remote: true do  %>
        <img src="/assets/icon-complete.png" alt="Complete">
      <% end %>
      <% if task.queue === 'quick' %>
        <a href="<%=  %>" method="POST" ><img src="/assets/icon-right.png" alt="MoveToGrass"></a>
      <% elsif task.queue === 'grass_catcher' %>
        <a href="#"><img src="/assets/icon-left.png" alt="MoveToQuick"></a>
      <% end %>
    <% end %>
    <%= link_to task_path(task), method: 'DELETE', remote: true do  %>
      <img src="/assets/icon-trash.png" alt="Remove">
    <% end %>
  </div>
</li>
